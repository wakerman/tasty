<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<data_type>csv</data_type>
	<pie>
		<x>260</x>
		<y>185</y>
		<radius>120</radius>
		<height>12</height>
		<angle>30</angle>
	</pie>

	<animation>
		<start_time>1</start_time>
		<start_effect>strong</start_effect>
		<pull_out_time>1</pull_out_time>
		<pull_out_only_one>true</pull_out_only_one>
	</animation>

	<data_labels>
		<show>
       <![CDATA[{title}: {percents}%]]>
		</show>
		<show_lines></show_lines>
		<line_color>#000000</line_color>
	</data_labels>

	<legend>
		<max_column>1</max_column>
		<x>450</x>
		<y>20</y>
		<width>240</width>
		<border_alpha>20</border_alpha>
		<spacing>9</spacing>
		<margins>5</margins>
		<align>center</align>
	</legend>

	<labels>
	</labels>
</settings>
